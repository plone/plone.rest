<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:five="http://namespaces.zope.org/five"
    xmlns:browser="http://namespaces.zope.org/browser"
    xmlns:plone="http://namespaces.plone.org/plone">

  <!-- Dexterity -->

  <plone:service
    method="GET"
    for="plone.dexterity.interfaces.IDexterityContent"
    factory=".demo.Get"
    />

  <plone:service
    method="POST"
    for="plone.dexterity.interfaces.IDexterityContent"
    factory=".demo.Post"
    />

  <plone:service
    method="PUT"
    for="plone.dexterity.interfaces.IDexterityContent"
    factory=".demo.Put"
    />

  <plone:service
    method="DELETE"
    for="plone.dexterity.interfaces.IDexterityContent"
    factory=".demo.Delete"
    />

  <plone:service
    method="PATCH"
    for="plone.dexterity.interfaces.IDexterityContent"
    factory=".demo.Patch"
    />

  <plone:service
    method="OPTIONS"
    for="plone.dexterity.interfaces.IDexterityContent"
    factory=".demo.Options"
    />

  <!-- Plone Site Root -->

  <plone:service
    method="GET"
    for="Products.CMFPlone.interfaces.IPloneSiteRoot"
    factory=".demo.Get"
    />

  <plone:service
    method="POST"
    for="Products.CMFPlone.interfaces.IPloneSiteRoot"
    factory=".demo.Post"
    />

  <plone:service
    method="PUT"
    for="Products.CMFPlone.interfaces.IPloneSiteRoot"
    factory=".demo.Put"
    />

  <plone:service
    method="DELETE"
    for="Products.CMFPlone.interfaces.IPloneSiteRoot"
    factory=".demo.Delete"
    />

  <plone:service
    method="PATCH"
    for="Products.CMFPlone.interfaces.IPloneSiteRoot"
    factory=".demo.Patch"
    />

  <plone:service
    method="OPTIONS"
    for="Products.CMFPlone.interfaces.IPloneSiteRoot"
    factory=".demo.Options"
    />

  <!-- Dexterity named services -->

  <plone:service
    method="GET"
    for="plone.dexterity.interfaces.IDexterityContent"
    factory=".demo.NamedGet"
    name="namedservice"
    />

  <plone:service
    method="POST"
    for="plone.dexterity.interfaces.IDexterityContent"
    factory=".demo.NamedPost"
    name="namedservice"
    />

  <plone:service
    method="PUT"
    for="plone.dexterity.interfaces.IDexterityContent"
    factory=".demo.NamedPut"
    name="namedservice"
    />

  <plone:service
    method="DELETE"
    for="plone.dexterity.interfaces.IDexterityContent"
    factory=".demo.NamedDelete"
    name="namedservice"
    />

  <plone:service
    method="PATCH"
    for="plone.dexterity.interfaces.IDexterityContent"
    factory=".demo.NamedPatch"
    name="namedservice"
    />

  <plone:service
    method="OPTIONS"
    for="plone.dexterity.interfaces.IDexterityContent"
    factory=".demo.NamedOptions"
    name="namedservice"
    />

  <!-- Error Page -->

  <plone:service
    method="GET"
    for="*"
    name="500-internal-server-error"
    factory=".testing.InternalServerErrorService"
    />

  <!-- CORS -->


  <plone:service
    method="POST"
    for="plone.dexterity.interfaces.IDexterityContent"
    factory=".demo.NamedOptions"
    name="corsexample"
    cors_origin="foobar"
    />

  <plone:service
    method="GET"
    for="plone.dexterity.interfaces.IDexterityContent"
    factory=".demo.NamedOptions"
    name="corsexample"
    cors_expose_all_headers="no"
    />


</configure>
